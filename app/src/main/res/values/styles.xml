    <style name="CustomCirclePageIndicator">
        <item name="fillColor">@color/color_select_tab</item>
        <item name="strokeColor">@color/color_default_tab</item>
        <item name="pageColor">@color/color_default_tab</item>
        <item name="strokeWidth">1dp</item>
        <item name="radius">5dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="centered">true</item>
    </style>

    <style name="CustomCirclePageIndicatorLight">
        <item name="fillColor">@color/color_select_tab_light</item>
        <!--<item name="strokeColor">@color/color_default_tab_light</item>-->
        <item name="pageColor">@color/pageColor_light</item>
        <item name="strokeWidth">1dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="radius">5dp</item>
        <item name="centered">true</item>
    </style>

